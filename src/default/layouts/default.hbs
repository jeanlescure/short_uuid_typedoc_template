<!doctype html>
<html class="default no-js toggle-inherited">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Short Unique ID - Short UUID Generator</title>
  <meta name="description" content="Documentation for {{project.name}}">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link rel="stylesheet" href="{{relativeURL "assets/css/main.css"}}">
  <script src="https://cdn.jsdelivr.net/npm/short-unique-id@latest/dist/short-unique-id.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.slim.min.js"></script>
  <script src="https://unpkg.com/cookiejs@1.0.26/dist/cookie.js"></script>
</head>
<body>

<div class="tsd-page-cookieconsent row">
  <div class="col-8">
    <p>This site uses cookies. To consent, continue navigating this site or click the "Accept cookies" button.</p>
  </div>
  <div class="col-4 consent-button-holder">
    <button class="cookieconsent-button">Accept cookies</button>
  </div>
</div>

{{> header}}

<div class="container container-main">
  <div class="row">
    <div class="col-3 col-menu menu-sticky-wrap menu-highlight">
      <h3>API Reference</h3>
      <nav class="tsd-navigation secondary menu-sticky">
        <ul class="before-current">
          {{#each toc.children}}
            {{> toc.root}}
          {{/each}}
        </ul>
      </nav>
    </div>
    <div class="col-9 col-content">
      {{{contents}}}
    </div>
  </div>
</div>

{{> footer}}

<div class="overlay"></div>
<script src="{{relativeURL "assets/js/main.js"}}"></script>
<script>if (location.protocol == 'file:') document.write('<script src="{{relativeURL "assets/js/search.js"}}"><' + '/script>');</script>
<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-159642755-1"></script>
<script>
  var gtagId = 'UA-159642755-1';
  window['ga-disable-' + gtagId] = true;

  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  function initTracking(){
    cookie('cookieconsent_status', 'allow');

    $('.tsd-page-cookieconsent').removeClass('show');
    $(window).off('scroll');
    $('.cookieconsent-button').off('click');

    window['ga-disable-' + gtagId] = false;
    gtag('event', 'CookieConsent');
    gtag('config', gtagId);
  }

  if (cookie.get('cookieconsent_status') === 'allow') {
    initTracking();
  } else {
    $('.tsd-page-cookieconsent').addClass('show');

    var scrollCounter = 0;
    $(window).scroll(function(e) {
      scrollCounter += 1;

      if (scrollCounter >= 30) {
        initTracking();
      }
    });

    $('.cookieconsent-button').click(function() {
      initTracking();
    });
  }
</script>

{{> analytics}}

</body>
</html>
